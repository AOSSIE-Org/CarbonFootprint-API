{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "arrow-parens": 0,
    "no-console": 0,
    "implicit-arrow-linebreak": 0,
    "no-plusplus": 0,
    "no-param-reassign": "off"
  }
}
